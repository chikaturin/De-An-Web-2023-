<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/Content/MasterLayout.css"/>
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <script src="https://kit.fontawesome.com/3e21e075ef.js" crossorigin="anonymous"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="above container-fluid d-flex" style="z-index:997">
        <div class="container above_header row-cols-auto">
            <header class="row  header1 col-xl-2 col-lg-4 col-sm-6">
                <a href="/Home"><img src="https://shopdunk.com/images/thumbs/0012445_Logo_ShopDunk.png" /></a>
            </header>
            <nav class="row">
                <div class="col-xl-1 col-sm-2 nav-item" style="margin-left:10px;">
                    <a class="text-decoration-none" href="@Url.Action("DetailPro/1", "Products")"><span style=" color: rgb(210,210,215); ">Iphone</span></a>
                </div>
                <div class="col-xl-1  col-sm-2">
                    <a class="text-decoration-none" href="@Url.Action("DetailPro/2", "Products")"><span style=" color: rgb(210,210,215);">iPad</span></a>
                </div>
                <div class="col-xl-1  col-sm-2">
                    <a class="text-decoration-none" href="@Url.Action("DetailPro/3", "Products")"><span style=" color: rgb(210,210,215); ">MachBook</span></a>
                </div>
                <div class="col-xl-1  col-sm-2">
                    <a class="text-decoration-none" href="@Url.Action("DetailPro/7", "Products")"><span style=" color: rgb(210,210,215); ">Watch</span></a>
                </div>
                <div class="col-xl-1  col-sm-2">
                    <a class="text-decoration-none" href="@Url.Action("DetailPro/4", "Products")"><span style=" color: rgb(210,210,215); ">Sounds</span></a>
                </div>
                <div class="col-xl-1  col-sm-2">
                    <a class="text-decoration-none" href="@Url.Action("DetailPro/5", "Products")"><span style=" color: rgb(210,210,215); ">Accessories</span></a>
                </div>
                <div class="col-xl-1  col-sm-2">
                    <a class="text-decoration-none" href="@Url.Action("DetailPro/6", "Products")"><span style=" color: rgb(210,210,215); "><span>Service <i class="fa-solid fa-angle-down" style="color: rgb(210,210,215);"></i></span></span></a>
                </div>
                <div class="col-xl-1  col-sm-2">
                    <a class="text-decoration-none" href="@Url.Action("DetailPro/6", "Products")"><span style=" color: rgb(210,210,215); "><span>News <i class="fa-solid fa-angle-down" style="color: rgb(210,210,215);"></i></span></span></a>
                </div>
            </nav>
            <header class="row col-xl-2 col-lg-4 col-sm-6">
                <div class="header2-div">
                    <i id="open_find" class="fa-solid fa-magnifying-glass" style="margin-top:1px;"></i>
                    <div>
                        <a href="@Url.Action("ShowCart","ShoppingCart")"><i class="fa-solid fa-bag-shopping"></i></a>
                        @Html.Action("BagCart","ShoppingCart")
                    </div>
                   
                    <i class="fa-regular fa-user" style="height:30px">
                        <span class="User" style="padding-top:7px;">
                            @if (Session["CusPhone"] != null)
                            {
                                <a href="@Url.Action("UserProfile", "LoginUser")"><p>Tài khoản của tôi</p></a>
                                <a href="@Url.Action("LogOutUser", "LoginUser")"><p>Log Out</p></a>
                            }
                            else
                            {
                                <a href="@Url.Action("RegisterCus", "LoginUser")"><p>Register</p></a>
                                <a href="@Url.Action("LoginCus", "LoginUser")"><p>Log In</p></a>
                            }
                        </span>
                    </i>
                    <img src="https://shopdunk.com/images/flags/vn.png" style="width:23px; height:20px; padding:4px 3px" />
                    <img src="https://shopdunk.com/images/flags/us.png" style="width:18px; height:13px; margin-top:3px;" />
                </div>
            </header>

        </div>
    </div>
    <div id="find" style="position: fixed; display: none; z-index: 997; width: 100%; height: 100%; top: 0; left: 0; background-color: rgba(0, 0, 0, 0.9);">
        <span id="searchx" style="color:red;font-weight:bolder;z-index:999;font-size:30px;float:right;margin-top:30px;;padding-right:20%;cursor:pointer">x</span>
        <div class=" justify-content-center align-items-center" style="height: 60vh;">
            <form style="width: 100%;" class="d-flex justify-content-center align-items-center flex-wrap" action="/products/search">
                <input style="width: 70%; margin-top:10%" type="search" class="form-control mx-2" id="search" name="search" value="@ViewBag.Search" placeholder="Search" maxlength=20 />
            </form>
            <a class="text-decoration-none d-flex justify-content-center mt-3" href="@Url.Action("SearchOption","Products")">Tìm kiếm theo giá sản phẩm</a>
        </div>
    </div>
    <div class="main">
        @RenderBody()
    </div>
    <footer class="footer-tb">
        <div class="Register justify-content-center">
            <div>
